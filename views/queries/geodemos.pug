block navigation
    +bcd-navbar("bcd-navbar","/","Cork Dashboard", "Queries")
    +queries__subnav("subnav")

block content
    .query
        .query__grid
            //- .query__tab-bar
                .query__tab-button tab 1
                .query__tab-button tab 2
            .query__source 
                p Sources:
                    a(href="https://rpubs.com/burcinwalsh/620510" target="_blank")
                       | RPubs
            .query__header
                +accordion({
                        id:"geodemos-header",
                        title: "Geodemographics", 
                        content: "<p>Geodemographic classification is a statistical tool that groups spatial units based on socio-economic characteristics.  This query follows the Ireland Geodemographic classification method with a focus on Cork.  Geographical Small Areas (SAs) are selected as spatial units and variables are taken from the 2016 Census of Population results.</p>",
                        classes: "accordion--active"
                        })
                        
            .query__dropdown-bar 
                p Select a group
                .query__dropdown All
            .query__help
                .query__help__icon i
                div 
                    p Choose a group to be visualised below on the map. Open the chart to see values. The heatmap shows an overview of statistics for all groups.
            .query__list list
                .query__list-item item 1
                .query__list-item item 2
                .query__list-item item 3
            .query__viz
                +accordion({
                    id: "geodemos-map",
                    title: "Groups Map",
                    content: "", 
                    classes: "accordion--active"
                    })
                        #map-geodemos.accordion__content 
                +accordion({
                    id: "geodemos-chart",
                    title: "Values Chart",
                    content: "", 
                    })
                        #chart-geodemos.accordion__content
                +accordion({
                    id: "geodemos-description",
                    title: "Group Description",
                    content: "", 
                    })
                    #geodemos-group-description.accordion__content.accordion__content__text 
                        .query__text__title title
                        p.query__text__text description: Lorem ipsum dolor, sit amet consectetur adipisicing elit. Tenetur iste praesentium consequuntur numquam a, vero eveniet reprehenderit quaerat voluptatum beatae iure ab fuga, ea architecto ipsum quae obcaecati, soluta molestiae!
                +accordion({
                    id: "geodemos-heamap",
                    title:"Heatmap"
                    })
